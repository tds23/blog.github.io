RewriteEngine On

RewriteBase /

RewriteCond %{HTTP_HOST} .
RewriteCond %{HTTP_HOST} https://tds23.github.io [NC]
RewriteRule (.*) https://tds23.github.io/$1 [R=301,L]

RewriteCond %{THE_REQUEST} ^[A-Z]{3,9}\ /([^/]+/)*index\.(htm(l)?|php)\ HTTP/
RewriteRule ^(([^/]+/)*)index\.(htm(l)?|php)$ https://tds23.github\.io/$1 [R=301,L]

RewriteCond %{HTTP_USER_AGENT} wget [NC]
RewriteRule .* - [F]